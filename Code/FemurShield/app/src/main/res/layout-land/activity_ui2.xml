<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/ui2rootLayout"
    tools:context="org.tbw.FemurShield.Controller.UI2">
    <FrameLayout
        android:id="@+id/sessionDetailsUI2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/separatorView"></FrameLayout>

    <View
        android:id="@+id/separatorView"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true" />

    <FrameLayout
        android:id="@+id/fallsListUI2"
        android:layout_toRightOf="@+id/sessionDetailsUI2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

</RelativeLayout>

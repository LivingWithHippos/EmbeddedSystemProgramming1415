<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ui3rootLayout"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context="org.tbw.FemurShield.Controller.UI3">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/separatorView"
        android:id="@+id/sessionDetailLayout">
    </FrameLayout>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/separatorView"
        android:layout_below="@+id/sessionDetailLayout"
        android:id="@+id/fallListLayout">
    </FrameLayout>

    <View
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:id="@+id/separatorView"/>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/commandLayout"
        android:layout_toRightOf="@+id/separatorView"
        android:id="@+id/sessionGraphLayout">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/commandLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/separatorView"
        android:layout_alignParentBottom="true">
    </FrameLayout>

</RelativeLayout>
